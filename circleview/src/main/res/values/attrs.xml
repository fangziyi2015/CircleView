<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircleView">
        <!--原图-->
        <attr name="src" format="reference" />
        <!--边框线的粗细-->
        <attr name="border_width" format="float" />
        <!--边框线的颜色-->
        <attr name="border_color" format="color" />
        <!--边框线的透明度-->
        <attr name="border_alpha" format="integer" />
        <!--边框破折线长度-->
        <attr name="border_dash_width" format="integer" />
        <!--是否显示边框-->
        <attr name="border_visibility" format="boolean" />
    </declare-styleable>
</resources>